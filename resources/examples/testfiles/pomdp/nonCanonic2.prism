pomdp
observable "middle" = s>0 & s<3;

module main
    s : [0..4] init 0;
    [] s=0 -> 0.5: (s'=1) + 0.5: (s'=2);
    [beta] s=1 -> 0.5: (s'=3) + 0.5: (s'=4);
    [alpha] s=2 -> 0.8: (s'=3) + 0.2: (s'=4);
    [beta] s=2 -> 0.4: (s'=3) + 0.6: (s'=4);
    [] s=3 | s=4 -> true;
endmodule

label "goal" = s=3;
